{
	"name": "c-cpp-run-anywhere",
	"displayName": "c/c++ run anywhere",
	"description": "let c or cpp run anywhere,without repeat config.",
	"version": "0.0.1",
	"publisher": "asheor",
	"icon": "https://raw.githubusercontent.com/Syclight/C-and-CPP-Run-Anywhere/master/test/icon/runAnywhere.bmp",
	"repository": {},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:c-c---run-anywhere.helloWorld",
		"onCommand:c-c---run-anywhere.Run",
		"onCommand:c-c---run-anywhere.Run_s",
		"onCommand:c-c---run-anywhere.Run_CS"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "c-c---run-anywhere.helloWorld",
				"title": "Hello"
			},
			{
				"command": "c-c---run-anywhere.Run",
				"title": "C&R C/C++"
			},
			{
				"command": "c-c---run-anywhere.Run_s",
				"title": "C&R C/C++ -s"
			},
			{
				"command": "c-c---run-anywhere.Run_CS",
				"title": "Compile C/C++ -S"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"	
	},
	"Dependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}